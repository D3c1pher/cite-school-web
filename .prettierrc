{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": false,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "objectWrap": "preserve",
  "arrowParens": "always",
  "plugins": ["prettier-plugin-tailwindcss"],
  "endOfLine": "auto"
}
