{
  "name": "cite-school-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "contributors": [
    {
      "name": "Norven Ephraim Caracas",
      "email": "norven.caracas@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/D3c1pher/cite-school-web.git"
  },
  "bugs": {
    "url": "https://github.com/D3c1pher/cite-school-web/issues"
  },
  "homepage": "https://github.com/D3c1pher/cite-school-web#readme",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx,json,css,scss,html}'",
    "format:check": "prettier --check 'src/**/*.{js,ts,jsx,tsx,json,css,scss,html}'",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "clean": "rimraf dist .next .turbo .swc coverage tsconfig.tsbuildinfo",
    "clean:all": "pnpm run clean && rimraf node_modules",
    "clean:install": "pnpm run clean:all && pnpm install"
  },
  "dependencies": {
    "next": "^15.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.14",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
